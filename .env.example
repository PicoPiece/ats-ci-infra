# ATS CI Infrastructure Environment Variables
# Copy this file to .env and fill in the actual values
# DO NOT commit .env to version control

# Jenkins Agent Configuration
# Get this secret from Jenkins UI: Manage Jenkins -> Manage Nodes -> Configure Agent -> Show Secret
JENKINS_SECRET=your-jenkins-agent-secret-here

# Grafana Configuration
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=change-this-password

# Optional: Prometheus Configuration
# PROMETHEUS_RETENTION=15d

# Optional: Network Configuration
# JENKINS_URL=http://jenkins:8080
